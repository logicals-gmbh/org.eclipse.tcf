            name="%activities.category.systemmanagement.name">
      </category>
      
      <activity
            description="%activities.activity.systemmanagement.perspective.description"
            id="org.eclipse.tcf.te.ui.views.activity.systemmanagement.perspective"
            name="%activities.activity.systemmanagement.perspective.name">
      </activity>
      <activity
            description="%activities.activity.systemmanagement.views.description"
            id="org.eclipse.tcf.te.ui.views.activity.systemmanagement.views"
            name="%activities.activity.systemmanagement.views.name">
      </activity>

      <activityPatternBinding
            activityId="org.eclipse.tcf.te.ui.views.activity.systemmanagement.perspective"
