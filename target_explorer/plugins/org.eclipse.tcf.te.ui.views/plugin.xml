      </categoryActivityBinding>
      
      <defaultEnablement
            id="org.eclipse.tcf.te.ui.views.activity.systemmanagement.perspective">
      </defaultEnablement>
      <defaultEnablement
            id="org.eclipse.tcf.te.ui.views.activity.systemmanagement.views">
      </defaultEnablement>
   </extension>
   
