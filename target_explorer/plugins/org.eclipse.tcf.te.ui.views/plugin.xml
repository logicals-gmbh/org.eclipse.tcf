      </activityPatternBinding>
      <activityPatternBinding
            activityId="org.eclipse.tcf.te.ui.views.activity.systemmanagement.views"
            pattern="org\.eclipse\.tcf\.te\.ui\.views/org\.eclipse\.tcf\.te\.ui\.views\.View">
      </activityPatternBinding>
      <activityPatternBinding
            activityId="org.eclipse.tcf.te.ui.views.activity.systemmanagement.views"
            pattern="org\.eclipse\.tcf\.te\.ui\.views/org\.eclipse\.tcf\.te\.ui\.views\.Editor">
      </activityPatternBinding>
      
      <categoryActivityBinding
